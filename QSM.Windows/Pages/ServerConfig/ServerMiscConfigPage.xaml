<?xml version="1.0" encoding="utf-8"?>
<Page
    x:Class="QSM.Windows.Pages.ServerConfig.ServerMiscConfigPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:QSM.Windows"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel Spacing="16">
        <TextBox Header="MOTD:" />
        <NumberBox Header="Player Idle Timeout (Minutes):" />
        <NumberBox Header="Rate Limit:" />
        <TextBox Header="Resource Pack:" />
        <TextBox Header="Resource Pack SHA1:" />
        <CheckBox Content="Require Resource Pack" />
    </StackPanel>
</Page>
